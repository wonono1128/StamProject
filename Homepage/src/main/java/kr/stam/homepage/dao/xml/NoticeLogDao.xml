<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.stam.homepage.dao.NoticeLogDao">
	<!-- 공지사항 입력 -->
	<insert id="insert" parameterType="kr.stam.homepage.dto.NoticeLogDto">
		insert into notice_log(noticeNum,managerId,managerName,logType,logTime) values(#{noticeNum},#{managerId}, #{managerName}, #{logType}, now())
	</insert>
</mapper>